      pos = ,
      self = ,
      name = ,
      class . struct = {
        name = ,
        kind = ,
        obs = ,
        pos = ,
        color = ,
        prereqset . struct = {
          prereq . struct = {
            starred = ,
            name = ,
            ang = ,
          }
        } ,
      } ,
    } ,
  }

