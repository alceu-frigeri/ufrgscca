\NewDocumentCommand{\addclass}{O{}D<>{}mmO{}}
  {
    \starray_new_term:nn {curricula.sem.class}{#3}
    \starray_set_from_keyval:nn {curricula.sem.class}
      {
        color = {#1} ,
        pos   = {#2} ,
        name  = {#3} ,
        kind  = {#4} ,
        obs   = {#5} ,
